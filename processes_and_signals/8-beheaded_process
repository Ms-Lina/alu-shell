#!/usr/bin/env bash
# Kill the "7-highlander" process.

# Find the PID of the "7-highlander" process
pid=$(pgrep -f "7-highlander")

if [ -n "$pid" ]; then
  # Use the `kill` command to terminate the process forcefully
  if kill -9 "$pid" 2>/dev/null; then
    echo "Killed the '7-highlander' process with PID $pid"
  else
    echo "Failed to kill the '7-highlander' process with PID $pid"
  fi
else
