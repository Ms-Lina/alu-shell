#!/usr/bin/env bash
# Stop the '4-to_infinity_and_beyond' process without using kill or killall.

# Use pkill to find and terminate the '4-to_infinity_and_beyond' process
pkill -f "4-to_infinity_and_beyond"

# Check if the process is still running
if ps -C "4-to_infinity_and_beyond" > /dev/null; then
  echo "Failed to stop '4-to_infinity_and_beyond' process"
else
  echo "Successfully stopped '4-to_infinity_and_beyond' process"
fi
